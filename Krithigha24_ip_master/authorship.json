tent":"    }"},{"lineNumber":65,"author":{"gitId":"Krithigha24"},"content":""},{"lineNumber":66,"author":{"gitId":"Krithigha24"},"content":""},{"lineNumber":67,"author":{"gitId":"-"},"content":"}"}],"authorContributionMap":{"Krithigha24":56,"-":11}},{"path":"src/main/java/Task.java","fileType":"java","lines":[{"lineNumber":1,"author":{"gitId":"Krithigha24"},"content":"public class Task {"},{"lineNumber":2,"author":{"gitId":"Krithigha24"},"content":"    protected String description;"},{"lineNumber":3,"author":{"gitId":"Krithigha24"},"content":"    protected boolean isDone;"},{"lineNumber":4,"author":{"gitId":"Krithigha24"},"content":""},{"lineNumber":5,"author":{"gitId":"Krithigha24"},"content":"    public Task(String description) {"},{"lineNumber":6,"author":{"gitId":"Krithigha24"},"content":"        this.description \u003d description;"},{"lineNumber":7,"author":{"gitId":"Krithigha24"},"content":"        this.isDone \u003d false;"},{"lineNumber":8,"author":{"gitId":"Krithigha24"},"content":"    }"},{"lineNumber":9,"author":{"gitId":"Krithigha24"},"content":""},{"lineNumber":10,"author":{"gitId":"Krithigha24"},"content":"    public String getStatusIcon() {"},{"lineNumber":11,"author":{"gitId":"Krithigha24"},"content":"        return (isDone ? \"[x] \" + description : \"[ ] \"+description); //return tick or X symbols"},{"lineNumber":12,"author":{"gitId":"Krithigha24"},"content":"    }"},{"lineNumber":13,"author":{"gitId":"Krithigha24"},"content":"    public void markAsDone(){"},{"lineNumber":14,"author":{"gitId":"Krithigha24"},"content":"        isDone \u003d true;"},{"lineNumber":15,"author":{"gitId":"Krithigha24"},"content":"    }"},{"lineNumber":16,"author":{"gitId":"Krithigha24"},"content":"}"}],"authorContributionMap":{"Krithigha24":16}}]
