         }"},{"lineNumber":32,"author":{"gitId":"Krithigha24"},"content":"            else if(echo.equals(\"list\")){"},{"lineNumber":33,"author":{"gitId":"Krithigha24"},"content":"                System.out.println(\"    ____________________________________________________________\");"},{"lineNumber":34,"author":{"gitId":"Krithigha24"},"content":"                for (int i \u003d 0; i \u003c count; i++) {"},{"lineNumber":35,"author":{"gitId":"Krithigha24"},"content":"                    System.out.print(\"     \"+(i+1));"},{"lineNumber":36,"author":{"gitId":"Krithigha24"},"content":"                    System.out.println(\". \"+store[i].getStatusIcon());"},{"lineNumber":37,"author":{"gitId":"Krithigha24"},"content":"                }"},{"lineNumber":38,"author":{"gitId":"Krithigha24"},"content":"                System.out.println(\"    ____________________________________________________________\");"},{"lineNumber":39,"author":{"gitId":"Krithigha24"},"content":"            }"},{"lineNumber":40,"author":{"gitId":"Krithigha24"},"content":"            else if(echo.contains(\"done\")) {"},{"lineNumber":41,"author":{"gitId":"Krithigha24"},"content":"                int value \u003d Integer.parseInt(echo.replaceAll(\"[^0-9]\", \"\"));"},{"lineNumber":42,"author":{"gitId":"Krithigha24"},"content":"                if (value \u003e count) {"},{"lineNumber":43,"author":{"gitId":"Krithigha24"},"content":"                    System.out.println(\"    ____________________________________________________________\");"},{"lineNumber":44,"author":{"gitId":"Krithigha24"},"content":"                    System.out.println(\"     Task \" + value + \" does not exist!\");"},{"lineNumber":45,"author":{"gitId":"Krithigha24"},"content":"                    System.out.println(\"    ____________________________________________________________\");"},{"lineNumber":46,"author":{"gitId":"Krithigha24"},"content":"                } else {"},{"lineNumber":47,"author":{"gitId":"Krithigha24"},"content"