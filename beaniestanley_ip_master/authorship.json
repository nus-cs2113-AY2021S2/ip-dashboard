t.println(\"\\t------------------------------------------\");"},{"lineNumber":47,"author":{"gitId":"beaniestanley"},"content":"                System.out.println(\"\\tSee you soon! Goodbye! ^.^\");"},{"lineNumber":48,"author":{"gitId":"beaniestanley"},"content":"                System.out.println(\"\\t__________________________________________\");"},{"lineNumber":49,"author":{"gitId":"beaniestanley"},"content":"                loop \u003d false;"},{"lineNumber":50,"author":{"gitId":"beaniestanley"},"content":"            } else {"},{"lineNumber":51,"author":{"gitId":"beaniestanley"},"content":"                // adds a task to the list of tasks"},{"lineNumber":52,"author":{"gitId":"beaniestanley"},"content":"                System.out.println(\"\\t------------------------------------------\");"},{"lineNumber":53,"author":{"gitId":"beaniestanley"},"content":"                System.out.println(\"\\tadded: \" + userInput);"},{"lineNumber":54,"author":{"gitId":"beaniestanley"},"content":"                Task task \u003d new Task(userInput);"},{"lineNumber":55,"author":{"gitId":"beaniestanley"},"content":"                tasks.add(task);"},{"lineNumber":56,"author":{"gitId":"beaniestanley"},"content":"                System.out.println(\"\\t__________________________________________\\n\");"},{"lineNumber":57,"author":{"gitId":"beaniestanley"},"content":"            }"},{"lineNumber":58,"author":{"gitId":"beaniestanley"},"content":"        }"},{"lineNumber":59,"author":{"gitId":"-"},"content":"    }"},{"lineNumber":60,"author":{"gitId":"-"},"content":"}"}],"authorContributionMap":{"beaniestanley":50,"-":10}},{"path":"src/main/java/Task.java","fileType":"java","lines":[{"lineNumber":1,"author":{"gitId":"beaniestanley"},"content":"public class Task {"},{"lineNumber":2,"author":{"gitId":"beaniestanley"},"content":"    protected String description;"},{"lineNumber":3,"author":{"gitId":"beaniestanley"},"content":"    protected boolean isDone;"},{"lineNumber":4,"author":{"gitId":"beaniestanley"},"content":""},{"lineNumber":5,"author":{"gitId":"beaniestanley"},"content":"    public Task(String description) {"},{"lineNumber":6,"author":{"gitId":"beaniestanley"},"content":"        this.description \u003d description;"},{"lineNumber":7,"author":{"gitId":"beaniestanley"},"content":"        this.isDone \u003d false;"},{"lineNumber":8,"author":{"gitId":"beaniestanley"},"content":"    }"},{"lineNumber":9,"author":{"gitId":"beaniestanley"},"content":""},{"lineNumber":10,"author":{"gitId":"beaniestanley"},"content":"    public String getStatusIcon() {"},{"lineNumber":11,"author":{"gitId":"beaniestanley"},"content":"        return (isDone ? \"\\u2713\" : \" \"); //return tick symbol if task is done"},{"lineNumber":12,"author":{"gitId":"beaniestanley"},"content":"    }"},{"lineNumber":13,"author":{"gitId":"beaniestanley"},"content":""},{"lineNumber":14,"author":{"gitId":"beaniestanley"},"content":"    public String getDescription() {"},{"lineNumber":15,"author":{"gitId":"beaniestanley"},"content":"        return this.description;"},{"lineNumber":16,"author":{"gitId":"beaniestanley"},"content":"    }"},{"lineNumber":17,"author":{"gitId":"beaniestanley"},"content":""},{"lineNumber":18,"author":{"gitId":"beaniestanley"},"content":"    public void markAsDone() {"},{"lineNumber":19,"author":{"gitId":"beaniestanley"},"content":"        this.isDone \u003d true;"},{"lineNumber":20,"author":{"gitId":"beaniestanley"},"content":"    }"},{"lineNumber":21,"author":{"gitId":"beaniestanley"},"content":"}"}],"authorContributionMap":{"beaniestanley":21}}]
