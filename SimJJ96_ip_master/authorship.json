content":"import java.util.List;"},{"lineNumber":3,"author":{"gitId":"SimJJ96"},"content":""},{"lineNumber":4,"author":{"gitId":"SimJJ96"},"content":"public class TaskManager {"},{"lineNumber":5,"author":{"gitId":"SimJJ96"},"content":"    private final List\u003cTask\u003e tasks \u003d new ArrayList\u003c\u003e();"},{"lineNumber":6,"author":{"gitId":"SimJJ96"},"content":"    private static final String BORDER \u003d \"\\t____________________________________________________________\\n\";"},{"lineNumber":7,"author":{"gitId":"SimJJ96"},"content":""},{"lineNumber":8,"author":{"gitId":"SimJJ96"},"content":"    public void toDo (String description) {"},{"lineNumber":9,"author":{"gitId":"SimJJ96"},"content":"        ToDo task \u003d new ToDo(description);"},{"lineNumber":10,"author":{"gitId":"SimJJ96"},"content":"        tasks.add(task);"},{"lineNumber":11,"author":{"gitId":"SimJJ96"},"content":"        printTaskMessage(task);"},{"lineNumber":12,"author":{"gitId":"SimJJ96"},"content":"    }"},{"lineNumber":13,"author":{"gitId":"SimJJ96"},"content":""},{"lineNumber":14,"author":{"gitId":"SimJJ96"},"content":"    public void deadLine (String description, String by) {"},{"lineNumber":15,"author":{"gitId":"SimJJ96"},"content":"        Deadline task \u003d new Deadline(description, by);"},{"lineNumber":16,"author":{"gitId":"SimJJ96"},"content":"        tasks.add(task);"},{"lineNumber":17,"author":{"gitId":"SimJJ96"},"content":"        printTaskMessage(task);"},{"lineNumber":18,"author":{"gitId":"SimJJ96"},"content":"    }"},{"lineNumber":19,"author":{"gitId":"SimJJ96"},"content":""},{"lineNumber":20,"author":{"gitId":"SimJJ96"},"content":"    public void event (String description, String at) {"},{"lineNumber":21,"author":{"gitId":"SimJJ96"},"content":"        Event task \u003d new Event(description, at);"},{"lineNumber":22,"author":{"gitId":"SimJJ96"},"content":"        tasks.add(task);"},{"lineNumber":23,"author":{"gitId":"SimJJ96"},"content":"        printTaskMessage(task);"}