meMessage() {"},{"lineNumber":36,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"Hello from\\n\" + LOGO);"},{"lineNumber":37,"author":{"gitId":"limwenfeng"},"content":"        printLine();"},{"lineNumber":38,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"Hello! I\u0027m Duke\");"},{"lineNumber":39,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"What can I do for you?\");"},{"lineNumber":40,"author":{"gitId":"limwenfeng"},"content":"    }"},{"lineNumber":41,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":42,"author":{"gitId":"limwenfeng"},"content":"    private static void addTasktoList(String[] userCommand) {"},{"lineNumber":43,"author":{"gitId":"limwenfeng"},"content":"        printLine();"},{"lineNumber":44,"author":{"gitId":"limwenfeng"},"content":"        tasksList[tasksCount] \u003d new Task(userCommand[0]);"},{"lineNumber":45,"author":{"gitId":"limwenfeng"},"content":"        tasksCount++;"},{"lineNumber":46,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"Added: \" + userCommand[0]);"},{"lineNumber":47,"author":{"gitId":"limwenfeng"},"content":"    }"},{"lineNumber":48,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":49,"author":{"gitId":"limwenfeng"},"content":"    private static void markTaskAsDone(String s) {"},{"lineNumber":50,"author":{"gitId":"limwenfeng"},"content":"        int taskNumber \u003d Integer.parseInt(s);"},{"lineNumber":51,"author":{"gitId":"limwenfeng"},"content":"        taskNumber--;"},{"lineNumber":52,"author":{"gitId":"limwenfeng"},"content":"        Task selectedTask \u003d tasksList[taskNumber];"},{"lineNumber":53,"author":{"gitId":"limwenfeng"},"content":"        selectedTask.markAsDone();"},{"lineNumber":54,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"Nice! Following task is now marked as done:\");"},{"lineNumber":55,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"[X] \" + selectedTask.description);"},{"lineNumber":56,"author":{"gitId":"limwenfeng"},"content":"    }"},{"lineNumber":57,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":58,"author":{"gitId":"limwenfeng"},"content":"    private static void exitDuke() {"},{"lineNumber":59,"author":{"gitId":"limwenfeng"},"content":"        printLine();"},{"lineNumber":60,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"Bye. Hope to see you again soon!\");"},{"lineNumber":61,"author":{"gitId":"limwenfeng"},"content":"    }"},{"lineNumber":62,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":63,"author":{"gitId":"limwenfeng"},"content":"    private static void printLine() {"},{"lineNumber":64,"author":{"gitId":"limwenfeng"},"content":"        System.out.println(\"---------------------------------------------------\");"},{"lineNumber":65,"author":{"gitId":"limwenfeng"},"content":"    }"},{"lineNumber":66,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":67,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":68,"author":{"gitId":"limwenfeng"},"content":"    private static String[] getUserInput() {"},{"lineNumber":69,"author":{"gitId":"limwenfeng"},"content":"        String userInput \u003d SCANNER.nextLine();"},{"lineNumber":70,"author":{"gitId":"limwenfeng"},"content":"        String[] listOfInputs \u003d userInput.split(\" \", 2);"},{"lineNumber":71,"author":{"gitId":"limwenfeng"},"content":"        if (!(listOfInputs[0].equalsIgnoreCase(\"done\" ))) {"},{"lineNumber":72,"author":{"gitId":"limwenfeng"},"content":"            listOfInputs[0] \u003d userInput;"},{"lineNumber":73,"author":{"gitId":"-"},"content":"        }"},{"lineNumber":74,"author":{"gitId":"limwenfeng"},"content":"        return listOfInputs;"},{"lineNumber":75,"author":{"gitId":"limwenfeng"},"content":"    }"},{"lineNumber":76,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":77,"author":{"gitId":"limwenfeng"},"content":""},{"lineNumber":78,"auth